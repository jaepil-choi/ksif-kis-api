{
  "mcpServers": {
    "toolbox": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@smithery/toolbox",
        "--key",
        "{{SMITHERY_KEY}}",
        "--profile",
        "{{SMITHERY_PROFILE}}"
      ]
    },
    "context7-mcp": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@upstash/context7-mcp",
        "--key",
        "{{SMITHERY_KEY}}",
        "--profile",
        "{{SMITHERY_PROFILE}}"
      ]
    },
    "mem0-memory-mcp": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@mem0ai/mem0-memory-mcp",
        "--key",
        "{{SMITHERY_KEY}}",
        "--profile",
        "{{SMITHERY_PROFILE}}"
      ]
    },
    "desktop-commander": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@wonderwhy-er/desktop-commander",
        "--key",
        "{{SMITHERY_KEY}}",
        "--profile",
        "{{SMITHERY_PROFILE}}"
      ]
    },
    "server-sequential-thinking": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@smithery-ai/server-sequential-thinking",
        "--key",
        "{{SMITHERY_KEY}}",
        "--profile",
        "{{SMITHERY_PROFILE}}"
      ]
    },
    "playwright-mcp": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@microsoft/playwright-mcp",
        "--key",
        "{{SMITHERY_KEY}}",
        "--profile",
        "{{SMITHERY_PROFILE}}"
      ]
    },
    "servers": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@jlia0/servers",
        "--key",
        "{{SMITHERY_KEY}}",
        "--profile",
        "{{SMITHERY_PROFILE}}"
      ]
    },
    "taskmaster-ai": {
      "command": "npx",
      "args": ["-y", "--package=task-master-ai", "task-master-ai"],      
      "env": {
        "ANTHROPIC_API_KEY": "{{ANTHROPIC_API_KEY}}",
        "PERPLEXITY_API_KEY": "{{PERPLEXITY_API_KEY}}",
        "OPENAI_API_KEY": "{{OPENAI_API_KEY}}",
        "GOOGLE_API_KEY": "{{GOOGLE_API_KEY}}",
        "MODEL": "claude-3-7-sonnet-20250219",
        "PERPLEXITY_MODEL": "sonar-pro",
        "MAX_TOKENS": 64000,
        "TEMPERATURE": 0.2,
        "DEFAULT_SUBTASKS": 5,
        "DEFAULT_PRIORITY": "medium"
      }
    }
  }
}